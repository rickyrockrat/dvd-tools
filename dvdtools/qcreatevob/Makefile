####### Install

install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/local/bin/" || $(MKDIR) "$(INSTALL_ROOT)/usr/local/bin/"
	-$(INSTALL_FILE) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/local/bin/"


install_trans: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/local/share/qcreatevob/lang/" || $(MKDIR) "$(INSTALL_ROOT)/usr/local/share/qcreatevob/lang/"
	-$(INSTALL_FILE) "translations/qcreatevob_de.qm" "$(INSTALL_ROOT)/usr/local/share/qcreatevob/lang/"
	-$(INSTALL_FILE) "translations/qcreatevob_ru.qm" "$(INSTALL_ROOT)/usr/local/share/qcreatevob/lang/"


uninstall_trans: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/local/share/qcreatevob/lang/qcreatevob_de.qm"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/local/share/qcreatevob/lang/qcreatevob_ru.qm"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/local/share/qcreatevob/lang/"


install_image: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/share/icons/" || $(MKDIR) "$(INSTALL_ROOT)/usr/share/icons/"
	-$(INSTALL_FILE) "desktop/qcreatevob-128x128.png" "$(INSTALL_ROOT)/usr/share/icons/"
	-$(INSTALL_FILE) "desktop/qcreatevob.png" "$(INSTALL_ROOT)/usr/share/icons/"


uninstall_image: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/icons/qcreatevob-128x128.png"
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/icons/qcreatevob.png"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/share/icons/"


install_menu: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/share/applnk/Multimedia/" || $(MKDIR) "$(INSTALL_ROOT)/usr/share/applnk/Multimedia/"
	-$(INSTALL_FILE) "desktop/qcreatevob.desktop" "$(INSTALL_ROOT)/usr/share/applnk/Multimedia/"


uninstall_menu: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/applnk/Multimedia/qcreatevob.desktop"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/share/applnk/Multimedia/"


install_images: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/local/share/qcreatevob/images/" || $(MKDIR) "$(INSTALL_ROOT)/usr/local/share/qcreatevob/images/"
	-$(INSTALL_FILE) "images/splash.png" "$(INSTALL_ROOT)/usr/local/share/qcreatevob/images/"


uninstall_images: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/local/share/qcreatevob/images/splash.png"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/local/share/qcreatevob/images/"


install: install_target install_trans install_image install_menu install_images  

uninstall: uninstall_target uninstall_trans uninstall_image uninstall_menu uninstall_images  

